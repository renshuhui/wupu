<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context="maptest.rsh.com.wupu.HomeActivity"
    android:orientation="vertical"
    >
        <FrameLayout
            android:layout_weight="1"
            android:id="@+id/home_AddFragment"
            android:layout_width="match_parent"
            android:layout_height="match_parent">
        </FrameLayout>

    <LinearLayout
        android:layout_weight="9"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:orientation="horizontal"
        >
        <ImageView
            android:id="@+id/home_shouye"
            android:layout_weight="1"
            android:layout_width="0dp"
            android:layout_height="match_parent"
            android:src="@drawable/a"
           android:clickable="true"
            />
        <ImageView
            android:id="@+id/home_huodong"
            android:layout_weight="1"
            android:layout_width="0dp"
            android:layout_height="match_parent"
            android:src="@drawable/b"
            android:clickable="true"
            />
        <ImageView
            android:id="@+id/home_shiping"
            android:layout_weight="1"
            android:layout_width="0dp"
            android:layout_height="match_parent"
            android:src="@drawable/c"
            android:clickable="true"
            />
        <ImageView
            android:id="@+id/home_quanzi"
            android:layout_weight="1"
            android:layout_width="0dp"
            android:layout_height="match_parent"
            android:src="@drawable/d"
            android:clickable="true"
            />
        <ImageView
            android:id="@+id/home_geren"
            android:layout_weight="1"
            android:layout_width="0dp"
            android:layout_height="match_parent"
            android:src="@drawable/e"
            android:clickable="true"
            />
    </LinearLayout>



</LinearLayout>
